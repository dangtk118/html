<jstranslator>    
    <validate-phone-numberonly-atleast7 translate="message" module="onestepcheckout">
        <message>Please enter a valid phone number. Minimum 7 numbers and no character.</message>
    </validate-phone-numberonly-atleast7>    
    <validate-number-only translate="message" module="onestepcheckout">
        <message>Please use numbers only in this field!</message>
    </validate-number-only>    
    <validate-length-minimum-100 translate="message" module="onestepcheckout">
        <message>Please enter at minimum 100 character!</message>
    </validate-length-minimum-100>
    <validate-billing-ctelephone  translate="message" module="onestepcheckout">
        <message>Please make sure your telephone match.</message>
    </validate-billing-ctelephone>
    <validate-shipping-ctelephone  translate="message" module="onestepcheckout">
        <message>Please make sure your telephone match.</message>
    </validate-shipping-ctelephone>
    <validate-maximum-length-100  translate="message" module="onestepcheckout">
        <message>Please enter at maximum 100 character!</message>
    </validate-maximum-length-100>
</jstranslator>
